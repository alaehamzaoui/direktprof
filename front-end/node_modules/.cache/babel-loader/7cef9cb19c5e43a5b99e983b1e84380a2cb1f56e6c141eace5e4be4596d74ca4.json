{"ast":null,"code":"import { profs } from \"../temp-data.js\";\nexport default {\n  name: \"ProfSeite\",\n  data() {\n    return {\n      prof: profs.find(prof => prof.id === this.$route.params.profId),\n      email: \"\",\n      topic: \"\",\n      showModal: false\n    };\n  },\n  methods: {\n    closeModal() {\n      this.showModal = false;\n    },\n    bookAppointment() {\n      alert(`Termin gebucht für ${this.prof.name} mit E-Mail: ${this.email} und Thema: ${this.topic}`);\n      this.showModal = false;\n      this.email = \"\";\n      this.topic = \"\";\n    }\n  }\n};","map":{"version":3,"names":["profs","name","data","prof","find","id","$route","params","profId","email","topic","showModal","methods","closeModal","bookAppointment","alert"],"sources":["C:\\Users\\hamza\\OneDrive\\Desktop\\S4\\WT2\\projekt\\direktprof\\front-end\\src\\pages\\ProfSeite.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-4\">\n    <div class=\"row\">\n      <div class=\"col-sm\">\n        <div class=\"card mb-4\">\n          <img\n            :src=\"prof.imageName\"\n            class=\"card-img-top rounded-circle\"\n            alt=\"Professor Bild\"\n          />\n          <div class=\"card-body bg-white text-danger\">\n            <!-- <h5 class=\"card-title\">{{ prof.name }}</h5>\n            <p class=\"card-text\">Sprechstunde: {{ prof.sprechstunde.tag }} von {{ prof.sprechstunde.beginn }} bis {{ prof.sprechstunde.ende }}</p>\n            -->\n            <div class=\"profile-card\">\n              <h5 class=\"card-title\">{{ prof.name }}</h5>\n              <p>Fachbereich Elektrotechnik und Informatik</p>\n              <p><i class=\"fas fa-phone\"></i> telefonnummer</p>\n              <p>\n                <i class=\"fas fa-map-marker-alt\"></i> Campus Bochum, Raum ...\n              </p>\n              <p>\n                <i class=\"fas fa-envelope\"></i>\n                vorname.nachname(at)hs-bochum.de\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-sm\">\n        <!--Hier sind die Termine zu sehen-->\n        <div class=\"appointments\">\n          <h4 class=\"h4-card-appointment-title\">Bitte wählen Sie einen Termin aus!</h4>\n          <div class=\"btn-grid\">\n            <div class=\"row\">\n              <div class=\"col\">\n                <!--Erste Auswahl-->\n                <div class=\"card-time\">\n                  <div class=\"card-header-time\">\n                    <h5 class=\"btn-date btn-primary m-2\">Do. 25.04.2024</h5>\n                  </div>\n                  <div class=\"card-body-time\">\n                    <button class=\"btn-time btn-primary m-2\">\n                      <tr>\n                        <td>{{ prof.sprechstunde.beginn }}</td>\n                        <td>-</td>\n                        <td>{{ prof.sprechstunde.ende }}</td>\n                      </tr>\n                    </button>\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"col\">\n                <!--Zweite Auswahl-->\n                <div class=\"card-time\">\n                  <div class=\"card-header-time\">\n                    <h5 class=\"btn-date btn-primary m-2\">Do. 25.04.2024</h5>\n                  </div>\n                  <div class=\"card-body-time\">\n                    <button class=\"btn-time btn-primary m-2\">\n                      <tr>\n                        <td>{{ prof.sprechstunde.beginn }}</td>\n                        <td>-</td>\n                        <td>{{ prof.sprechstunde.ende }}</td>\n                      </tr>\n                    </button>\n                  </div>\n                </div>\n              </div>\n              <div class=\"w-100\"></div>\n              <div class=\"col\">\n                <!--dritte Auswahl-->\n                <div class=\"card-time\">\n                  <div class=\"card-header-time\">\n                    <h5 class=\"btn-date btn-primary m-2\">Do. 25.04.2024</h5>\n                  </div>\n                  <div class=\"card-body-time\">\n                    <button class=\"btn-time btn-primary m-2\">\n                      <tr>\n                        <td>{{ prof.sprechstunde.beginn }}</td>\n                        <td>-</td>\n                        <td>{{ prof.sprechstunde.ende }}</td>\n                      </tr>\n                    </button>\n                  </div>\n                </div>\n              </div>\n              <div class=\"col\">\n                <!--vierte Auswahl-->\n                <div class=\"card-time\">\n                  <div class=\"card-header-time\">\n                    <h5 class=\"btn-date btn-primary m-2\">Do. 25.04.2024</h5>\n                  </div>\n                  <div class=\"card-body-time\">\n                    <button class=\"btn-time btn-primary m-2\">\n                      <tr>\n                        <td>{{ prof.sprechstunde.beginn }}</td>\n                        <td>-</td>\n                        <td>{{ prof.sprechstunde.ende }}</td>\n                      </tr>\n                    </button>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"row\">\n              <div class=\"col\">\n                <button \n                  class=\"btn-buchen btn-primary m-2\"\n                  @click=\"showModal = true\"\n                >\n                  <h5>Auswählen</h5>\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- hier muss das design bearbeitet werden \n     <table class=\"table table-striped bg-white text-danger\">\n       <thead class=\"bg-danger text-white\">\n         <tr>\n           <th scope=\"col\">Tag</th>\n           <th scope=\"col\">Beginn</th>\n           <th scope=\"col\">Ende</th>\n           <th scope=\"col\">Aktion</th>\n\n         </tr>\n       </thead>\n       <tbody>\n         <tr>\n           <td>{{ prof.sprechstunde.tag }}</td>\n           <td>{{ prof.sprechstunde.beginn }}</td>\n           <td>{{ prof.sprechstunde.ende }}</td>\n           <td><button class=\"btn btn-outline-danger\" @click=\"showModal = true\">Buchen</button></td>\n         </tr>\n       </tbody>\n     </table>\n\n     -->\n\n      <!-- Auswähen button -->\n    </div>\n\n    <!-- Modal -->\n    <div\n      v-if=\"showModal\"\n      class=\"modal fade show d-block\"\n      tabindex=\"-1\"\n      role=\"dialog\"\n      aria-labelledby=\"bookingModalLabel\"\n      aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header \">\n            <h5 class=\"modal-title\" id=\"bookingModalLabel\">Bitte Formular ausfüllen!</h5>\n            <button\n              type=\"button\"\n              class=\"close\"\n              @click=\"closeModal\"\n              aria-label=\"Close\"\n            >\n              <span aria-hidden=\"true\">&times;</span>\n            </button>\n          </div>\n          <div class=\"modal-body\">\n            <form @submit.prevent=\"bookAppointment\">\n              <div class=\"form-group\">\n                <label for=\"email\">E-Mail</label>\n                <input\n                  type=\"email\"\n                  class=\"form-control\"\n                  id=\"email\"\n                  v-model=\"email\"\n                  required\n                />\n              </div>\n              <div class=\"form-group\">\n                <label for=\"topic\">Anliegen</label>\n                <input\n                  type=\"text\"\n                  class=\"form-control\"\n                  id=\"topic\"\n                  v-model=\"topic\"\n                  required\n                />\n              </div>\n              <div>\n                <button type=\"submit\" class=\"btn-senden\">Senden</button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div v-if=\"showModal\" class=\"modal-backdrop fade show\"></div>\n  </div>\n</template>\n\n<script>\nimport { profs } from \"../temp-data.js\";\n\nexport default {\n  name: \"ProfSeite\",\n  data() {\n    return {\n      prof: profs.find((prof) => prof.id === this.$route.params.profId),\n      email: \"\",\n      topic: \"\",\n      showModal: false,\n    };\n  },\n  methods: {\n    closeModal() {\n      this.showModal = false;\n    },\n    bookAppointment() {\n      alert(\n        `Termin gebucht für ${this.prof.name} mit E-Mail: ${this.email} und Thema: ${this.topic}`\n      );\n      this.showModal = false;\n      this.email = \"\";\n      this.topic = \"\";\n    },\n  },\n};\n</script>\n\n<style>\n/* .appointment{\n }*/\nheader {\n  background-color: red; /* Red background color */\n  padding: 0%;\n  /*padding-bottom: 0%;*/\n  margin: 0;\n  text-align: center;\n  height: 50px;\n}\n\n.header-container {\n  margin-right: 0;\n  margin-left: auto;\n}\n\n.header-title {\n  font-size: 2rem;\n  color: #ffffff;\n  float: right;\n}\n\n.container {\n  color: black;\n  \n}\n.card-body {\n  background-color: white;\n}\n.profile-card{\n  color: red;\n  \n  \n}\n\n/* Karten-Stile */\n.h4-card-appointment-title{\n  margin-left: 20%;\n}\n\n.btn-grid{\n  \n\n  display: flex; /* Arrange professor cards horizontally */\n  flex-wrap: wrap; /* Wrap cards to fit container width */\n  justify-content: center;\n \n}\n\n.card-time {\n  /*border: 1px solid black;*/\n  \n  border-radius: 5px;\n  margin: 20px;\n  padding: 10px;\n  box-shadow: 10px 4px 20px rgb(104, 97, 97);\n  border-radius: 20px;\n\n\n}\n\n.card-header-time {\n  color: white;\n  padding: 10px;\n  padding-left: 0%;\n  border-top-left-radius: 5px;\n  border-top-right-radius: 5px;\n  margin-bottom: 2%;\n}\n\n.card-body-time {\n  background-color: white;\n  padding: 10px;\n  padding-left: 0%;\n}\n.btn-date {\n  color: black;\n  box-shadow: 10px 4px 20px rgb(104, 97, 97);\n\n  border-color: gray;\n  background-color: #bdb6b6;\n  width: 100%;\n  margin-left: 0%;\n}\n.btn-time {\n  color: white;\n  width: 100%;\n  background-color: rgb(9, 156, 9);\n  box-shadow: 10px 4px 20px rgb(104, 97, 97);\n\n  border-color: rgb(9, 156, 9);\n  border-block-color: none;\n\n \n}\n\n.time-slot {\n  margin-bottom: 10px;\n}\n.btn-buchen{\n  width: 80%;\n  height: 80%;\n  margin-left: 20%;\n\n  display: flex; /* Arrange professor cards horizontally */\n  flex-wrap: wrap; /* Wrap cards to fit container width */\n  justify-content: center;\n  \n  \n}\n\n.action-button {\n  text-align: center;\n  margin-top: 10px;\n}\n\n.modal-dialog{\n  border-radius: 30%;\n  box-shadow: 10px 4px 15px rgb(104, 97, 97);\n\n\n}\n.close{\n  position: absolute;\n  right: 0;\n  margin-right: 5%;\n  border-radius: 40%;\n  color: black;\n  border-color: white;\n  background-color: white;\n\n}\n.modal-header{\n  color: white;\n  background-color: #bdb6b6;\n\n}\n.btn-senden{\n  background-color: red;\n  color: white;\n\n  border-color: red;\n  border-radius: 10px;\n  height:50%;\n  width: 20%;\n  margin-top: 3%;\n  margin-left: auto;\n  margin-right: auto;\n  justify-content: center;\n  align-items: center;\n  display: flex;\n  box-shadow: 10px 4px 15px rgb(104, 97, 97);\n\n\n}\n\n</style>\n"],"mappings":"AA2MA,SAASA,KAAI,QAAS,iBAAiB;AAEvC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAEH,KAAK,CAACI,IAAI,CAAED,IAAI,IAAKA,IAAI,CAACE,EAAC,KAAM,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,MAAM,CAAC;MACjEC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDG,eAAeA,CAAA,EAAG;MAChBC,KAAK,CACF,sBAAqB,IAAI,CAACZ,IAAI,CAACF,IAAI,gBAAgB,IAAI,CAACQ,KAAK,eAAe,IAAI,CAACC,KAAM,EAC1F,CAAC;MACD,IAAI,CAACC,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACF,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,KAAI,GAAI,EAAE;IACjB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDe